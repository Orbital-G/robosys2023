def plus(*numbers):
    ans = 0
    for i in numbers:
        ans += i
    return ans

def main():
    print("足したい数を入力")
    numbers = [int(line) for line in input().split()]

    result = plus(*numbers)

    print(f"{numbers} の合計は {result} です。")

if __name__ == "__main__":
  main()
